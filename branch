if num <= 12:
    print('ticket price $10')
elif num <= 64:
    print('ticket for $20')
else:
    print('ticket is $15 if age is greater than 65 ')



x = int(input('enter No.'))
y = int(input('enter another No.'))
z = int(input('enter another No.'))

if x < y and x < z:
   print(x,'is the lowest')
elif y < x and y < z:
   print(y, 'is the lowest')
else:
    print(z,'is the lowest')

if x > y and x < z:
    print(x,'is the second lowest')
elif y > x and y < z or y < x and y > z:
    print(y, 'is the lowest number')
else:
    print(z, 'is the second lowest number')


